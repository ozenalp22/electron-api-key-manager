{
  "name": "electron-api-key-manager",
  "version": "1.0.0",
  "description": "A secure API key management application for macOS",
  "main": "dist/main/main.js",
  "homepage": "https://github.com/yourusername/electron-api-key-manager",
  "author": {
    "name": "Your Name",
    "email": "your.email@example.com"
  },
  "license": "MIT",
  "private": true,
  "keywords": [
    "electron",
    "api-keys",
    "security",
    "macos",
    "keychain",
    "typescript"
  ],
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "preview": "electron-vite preview",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:mac-arm64": "electron-vite build && electron-builder --mac --arm64",
    "build:mac-x64": "electron-vite build && electron-builder --mac --x64",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "electron-store": "^10.0.0",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
